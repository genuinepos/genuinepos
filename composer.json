{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "consoletvs/charts": "6.5.6",
        "doctrine/dbal": "^3.5.1",
        "fruitcake/laravel-cors": "^2.2.0",
        "guzzlehttp/guzzle": "^7.5.0",
        "innocenzi/laravel-vite": "0.2.*",
        "intervention/image": "^2.7.2",
        "laravel/framework": "^9.3.10",
        "laravel/tinker": "^2.7.2",
        "laravel/ui": "^3.4.6",
        "maatwebsite/excel": "^3.1.44",
        "milon/barcode": "^9.0.1",
        "picqer/php-barcode-generator": "^2.2.4",
        "spatie/laravel-permission": "^5.5",
        "yajra/laravel-datatables": "1.5.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "fakerphp/faker": "^1.20.0",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^6.3.1",
        "phpunit/phpunit": "^9.5.26",
        "spatie/laravel-ignition": "^1.6.1",
        "kitloong/laravel-migrations-generator": "^5.0",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "thedoctor0/laravel-factory-generator": "^1.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories" : [
        {
            "type" : "path",
            "url" : "packages/installer"
        }
    ]
}
